
{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
      "airbnb-typescript",
      "prettier/@typescript-eslint",
      "plugin:prettier/recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "prettier",
        "@typescript-eslint",
        "jest"
    ],
    "rules": {
        // "camelcase": "off"
    }
}